{
  "env": {
    "browser": true,
    "es2021": true,
    "cypress/globals": true
  },
  "extends": ["eslint:recommended", "airbnb", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react","cypress"],
  "rules": {
    "no-plusplus": "off",
    "no-alert": "off"
  }
}
